Keywords = {SOFTWARE analytics, ORGANIZATIONAL performance, INFORMATION technology, STRATEGIC planning, ECONOMIC competition, Data analytics use, Data-tools fit, Firm agility, People-tools fit, Tasks-tools fit},
Pages = {95 - 105},

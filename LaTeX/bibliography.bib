Journal = {Decision Support Systems},
Keywords = {BIG data, COVID-19, COVID-19 pandemic, DECISION making, Big data analytics capability, Decision quality, Decision speed, Event criticality of COVID-19, Event disruption of COVID-19, IT-business alignment},
Pages = {N.PAG},
Title = {IT-business alignment, big data analytics capability, and strategic decision-making: Moderating roles of event criticality and disruption of COVID-19.},
Volume = {161},
